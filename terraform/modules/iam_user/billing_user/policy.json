{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket",
        "s3:GetObject"
      ],
      "Resource": [
        "arn:${aws_partition}:s3:::${billing_bucket}",
        "arn:${aws_partition}:s3:::${billing_bucket}/*"
      ]
    }
  ]
}
