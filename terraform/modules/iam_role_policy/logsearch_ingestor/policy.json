{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:DeleteObject",
        "s3:GetObject",
        "s3:ListObjects",
        "s3:ListObjectsV2",
        "s3:ListObjectVersions"
      ],
      "Resource": [
        "arn:${aws_partition}:s3:::logsearch-*/*"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "logs:DescribeLogGroups",
        "logs:DescribeLogStreams"
      ],
      "Resource": [
        "arn:${aws_partition}:logs:${aws_default_region}:${account_id}:*"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:PutLogEvents"
      ],
      "Resource": [
        "arn:${aws_partition}:logs:${aws_default_region}:${account_id}:log-group:logsearch-*"
      ]
    }
  ]
}
